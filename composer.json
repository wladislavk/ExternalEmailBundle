    "keywords": ["email", "wildcards", "symfony2", "symfony3"],
    "homepage": "https://github.com/wladislavk/ExternalEmailBundle",
    "license": "MIT",
    "authors": [
        {"name": "Vladislav Kryshtanovskiy", "email": "wladislav.k@gmail.com"}
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~2.8|~3.0",
        "vkr/settings-bundle": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "VKR\\ExternalEmailBundle\\": ""
        }
    }
}
